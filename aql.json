items.find({
  "name": {
    "$match": "spring-petclinic*.jar"
  },
  "$and": [
    {
      "@build.name": {
        "$eq": "Petclinic-BuildAndUpload"
      }
    },
    {
      "repo": {
        "$eq": "maven-release-local"
      }
    }
  ]
}).include("created").sort({
  "$desc": [
    "created"
  ]
}).limit(1)



